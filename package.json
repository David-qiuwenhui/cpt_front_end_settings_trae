{
  "name": "cpt_front_end_settings_trae",
  "version": "1.0.0",
  "description": "国际化配置管理工程",
  "type": "module",
  "main": "i18n/src/main.js",
  "scripts": {
    "dev": "node --experimental-modules i18n/src/main.js",
    "generate:excel": "node --experimental-modules i18n/src/scripts/generateExcel.js",
    "parse:excel": "node --experimental-modules i18n/src/scripts/parseExcel.js",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:verbose": "vitest run --reporter=verbose",
    "test:scripts": "vitest run --reporter=verbose i18n/src/scripts/__tests__/*.test.js",
    "test:main": "vitest run --reporter=verbose i18n/src/__tests__/*.test.js",
    "prepare": "husky install"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "excel"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "xlsx": "^0.18.5",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "vitest": "^1.1.3"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "node --experimental-modules i18n/src/scripts/validateKeys.js"
    ]
  }
}